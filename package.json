{
  "name": "simple-weather-app",
  "version": "0.1.0",
  "homepage": "https://woozzone.github.io/simple-weather-app",
  "private": true,
  "dependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "normalize.css": "^8.0.0",
    "prettier": "^1.13.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
